{
  "version": "0.0.0",
  "name": "@fly-lab/template-react-tsup",
  "author": "DHN Chandan",
  "license": "MIT",
  "main": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    "default": "./dist/index.cjs",
    "require": "./dist/index.cjs",
    "import": "./dist/index.cjs",
    "node": "./dist/index.cjs"
  },
  "type": "module",
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fly-lab/template-react-tsup.git"
  },
  "bugs": {
    "email": "fly.dev.org@gmail.com",
    "url": "https://github.com/fly-lab/template-react-tsup/issues"
  },
  "homepage": "https://github.com/fly-lab/template-react-tsup.git",
  "keywords": [
    "typescript",
    "library",
    "npm",
    "tsup",
    "react"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup -- --watch",
    "prepare": "husky install",
    "cz": "git cz",
    "cz:install": "commitizen init cz-conventional-changelog --yarn --dev --exact",
    "cz:force": "commitizen init cz-conventional-changelog --yarn --dev --exact --force"
  },
  "devDependencies": {
    "@fly-lab/eslint-config": "workspace:*",
    "@fly-lab/tsconfig": "workspace:*",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "^18.2.0",
    "terser": "^5.16.8",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
